<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@drawable/house"
     android:layout_height="match_parent"
    >
    <EditText
        android:layout_width="320dp"
        android:layout_height="50dp"
        android:layout_marginTop="50dp"
        android:layout_centerHorizontal="true"
        android:gravity="right"
        android:textSize="30dp"
        android:background="#10000000"
        android:cursorVisible="false"
        android:id="@+id/et_input"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:layout_alignParentBottom="true"
        android:id="@+id/bottom"
        />
    <LinearLayout
        android:layout_width="320dp"
        android:layout_height="80dp"
        android:layout_centerHorizontal="true"
        android:layout_above="@+id/bottom"
        android:id="@+id/line4"
        >
        <Button
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_margin="5dp"
            android:background="#20000000"
            android:text="."
            android:textSize="30sp"
            android:textColor="#000"
            android:gravity="center"
            android:id="@+id/btpoint"
            />

        <Button
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_margin="5dp"
            android:background="#20000000"
            android:text="0"
            android:textSize="25dp"
            android:textColor="#FAFAD2"
            android:gravity="center"
            android:id="@+id/bt0"
            />

        <Button
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_margin="5dp"
            android:background="#20000000"
            android:text="DEL"
            android:textSize="30sp"
            android:textColor="#FAFAD2"
            android:gravity="center"
            android:id="@+id/btDEL"
            />
        <Button
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_margin="5dp"
            android:background="#20000000"
            android:text="="
            android:textSize="25dp"
            android:textColor="#FAFAD2"
            android:gravity="center"
            android:id="@+id/btdeng"
            />

    </LinearLayout>
    <LinearLayout
    android:layout_width="320dp"
    android:layout_height="80dp"
    android:layout_centerHorizontal="true"
    android:layout_above="@+id/line4"
    android:id="@+id/line3"
    >

        <Button
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_margin="5dp"
            android:background="#20000000"
            android:text="1"
            android:textSize="30sp"
            android:textColor="#FAFAD2"
            android:gravity="center"
            android:id="@+id/bt1" />

        <Button
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_margin="5dp"
        android:background="#20000000"
            android:text="2"
            android:textSize="30sp"
            android:textColor="#FAFAD2"
            android:gravity="center"
            android:id="@+id/bt2"
            />
    <Button
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_margin="5dp"
        android:background="#20000000"
        android:text="3"
        android:textSize="30sp"
        android:textColor="#FAFAD2"
        android:gravity="center"
        android:id="@+id/bt3"
        />
    <Button
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_margin="5dp"
        android:background="#20000000"
        android:text="+"
        android:textSize="30sp"
        android:textColor="#FAFAD2"
        android:gravity="center"
        android:id="@+id/btjia"
        />

</LinearLayout>
    <LinearLayout
        android:layout_width="320dp"
        android:layout_height="80dp"
        android:layout_centerHorizontal="true"
        android:layout_above="@+id/line3"
        android:id="@+id/line2"
        >
        <Button
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_margin="5dp"
            android:background="#20000000"
            android:text="4"
            android:textSize="30sp"
            android:textColor="#FAFAD2"
            android:gravity="center"
            android:id="@+id/bt4"
            />
        <Button
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_margin="5dp"
            android:background="#20000000"
            android:text="5"
            android:textSize="30sp"
            android:textColor="#FAFAD2"
            android:gravity="center"
            android:id="@+id/bt5"
            />
        <Button
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_margin="5dp"
            android:background="#20000000"
            android:text="6"
            android:textSize="30sp"
            android:textColor="#FAFAD2"
            android:gravity="center"
            android:id="@+id/bt6"
            />
        <Button
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_margin="5dp"
            android:background="#20000000"
            android:text="-"
            android:textSize="30sp"
            android:textColor="#FAFAD2"
            android:gravity="center"
            android:id="@+id/btjian"
            />

    </LinearLayout>
    <LinearLayout
        android:layout_width="320dp"
        android:layout_height="80dp"
        android:layout_centerHorizontal="true"
        android:layout_above="@+id/line2"
        android:id="@+id/line1"
        >
        <Button
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_margin="5dp"
            android:background="#20000000"
            android:text="7"
            android:textSize="30sp"
            android:textColor="#FAFAD2"
            android:gravity="center"
            android:id="@+id/bt7"
            />
        <Button
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_margin="5dp"
            android:background="#20000000"
            android:text="8"
            android:textSize="30sp"
            android:textColor="#FAFAD2"
            android:gravity="center"
            android:id="@+id/bt8"
            />
        <Button
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_margin="5dp"
            android:background="#20000000"
            android:text="9"
            android:textSize="30sp"
            android:textColor="#FAFAD2"
            android:gravity="center"
            android:id="@+id/bt9"
            />
        <Button
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_margin="5dp"
            android:background="#20000000"
            android:text="*"
            android:textSize="30sp"
            android:textColor="#FAFAD2"
            android:gravity="center"
            android:id="@+id/btcheng"
            />

    </LinearLayout>
    <LinearLayout
    android:layout_width="320dp"
    android:layout_height="80dp"
    android:layout_centerHorizontal="true"
    android:layout_above="@+id/line1"
    android:id="@+id/line0"
    >
    <Button
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_margin="5dp"
        android:background="#20000000"
        android:text="("
        android:textSize="30sp"
        android:textColor="#FAFAD2"
        android:gravity="center"
        android:id="@+id/btzuo"
        />
    <Button
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_margin="5dp"
        android:background="#20000000"
        android:text=")"
        android:textSize="30sp"
        android:textColor="#FAFAD2"
        android:gravity="center"
        android:id="@+id/btyou"
        />
    <Button
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_margin="5dp"
        android:background="#20000000"
        android:text="C"
        android:textSize="30sp"
        android:textColor="#FAFAD2"
        android:gravity="center"
        android:id="@+id/btC"
        />
    <Button
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_margin="5dp"
        android:background="#20000000"
        android:text="รท"
        android:textSize="30sp"
        android:textColor="#FAFAD2"
        android:gravity="center"
        android:id="@+id/btchu"
        />

</LinearLayout>
</RelativeLayout>
